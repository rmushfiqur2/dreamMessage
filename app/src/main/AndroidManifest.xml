<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cloud.mushfiq.buet.dreamsychology">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".inbox"
            android:label="@string/title_activity_inbox"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".VerifyActivity" />
        <activity
            android:name=".chat_user"
            android:label="@string/title_activity_chat_user"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".NameActivity" />
        <activity
            android:name=".inbox_vol"
            android:label="@string/title_activity_inbox"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ManageActivity"
            android:label="@string/title_activity_manage" />
        <activity android:name=".About" />
        <activity
            android:name=".EditSavedReply"
            android:label="Saved Replies" />
        <activity android:name=".starting" />
        <activity android:name=".Web" />
        <activity android:name=".Appoint" />
        <activity android:name=".userlist" android:label="All users"/>
        <service
            android:name=".MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT"/>
            </intent-filter>
        </service>
        <service
            android:name=".MyFirebaseInstanceIDService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT"/>
            </intent-filter>
        </service>
        <activity
            android:name=".LoginActivity"
            android:label="Log in" />
    </application>

</manifest>